();\n  });\n};\nexport var logiCancel = function logiCancel(data) {\n  return fetch(\"/api/cancel/logic\", raw(data, \"POST\")).then(function (response) {\n    return response.json();\n  });\n};\nexport var lenguaCancel = function lenguaCancel(data) {\n  return fetch(\"/api/cancel/lengua\", raw(data, \"POST\")).then(function (response) {\n    return response.json();\n  });\n};\nexport var examCancel = function examCancel(data) {\n  return fetch(\"/api/cancel/exam\", raw(data, \"POST\")).then(function (response) {\n    return response.json();\n  });\n}; //destroy\n\nexport var allCancel = function allCancel(data) {\n  return fetch(\"/api/cancel/all\", raw(data, \"POST\")).then(function (response) {\n    return response.json();\n  });\n};","map":{"version":3,"sources":["C:/proyectos/next/seani/utils/api.js"],"names":["host","myHeaders","Headers","append","raw","data","tipe","method","headers","body","JSON","stringify","redirect","logIn","fetch","then","response","json","getInitialData","getUser","dat","openSocket","testExam","testLogic","lenguaExam","mateExam","mateCancel","logiCancel","lenguaCancel","examCancel","allCancel"],"mappings":"AAAA,SAASA,IAAT,QAAqB,aAArB;AACA,OAAO,kBAAP;AACA,IAAIC,SAAS,GAAG,IAAIC,OAAJ,EAAhB;AACAD,SAAS,CAACE,MAAV,CAAiB,cAAjB,EAAiC,kBAAjC,E,CACA;;AAEA,IAAMC,GAAG,GAAG,SAANA,GAAM,CAACC,IAAD,EAAOC,IAAP,EAAgB;AAC1B,MAAI,CAACA,IAAL,EAAW;AACT,WAAO,EAAP;AACD;;AACD,SAAO;AACLC,IAAAA,MAAM,EAAED,IADH;AAELE,IAAAA,OAAO,EAAEP,SAFJ;AAGLQ,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeN,IAAf,CAHD;AAILO,IAAAA,QAAQ,EAAE;AAJL,GAAP;AAMD,CAVD;;AAYA,OAAO,IAAMC,KAAK,GAAE,SAAPA,KAAO,CAACR,IAAD,EAAQ;AACxB,SAAOS,KAAK,aAAaV,GAAG,CAACC,IAAD,EAAM,MAAN,CAAhB,CAAL,CAAoCU,IAApC,CAAyC,UAAAC,QAAQ;AAAA,WAAEA,QAAQ,CAACC,IAAT,EAAF;AAAA,GAAjD,CAAP;AACH,CAFM,C,CAGP;;AACA,OAAO,IAAMC,cAAc,GAAE,SAAhBA,cAAgB,GAAI;AAC7B,SAAOJ,KAAK,WAAId,IAAJ,kBAAL,CAA6Be,IAA7B,CAAkC,UAAAC,QAAQ;AAAA,WAAEA,QAAQ,CAACC,IAAT,EAAF;AAAA,GAA1C,CAAP;AACH,CAFM,C,CAGP;;AACA,OAAO,IAAME,OAAO,GAAE,SAATA,OAAS,CAACd,IAAD,EAAQ;A